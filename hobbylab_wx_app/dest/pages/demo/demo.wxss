/**index.wxss**/
.pagemain .location {
  padding-top: 15px;
  padding-bottom: 17px;
  font-size: 13px;
  color: #ff5669; }

.pagemain .list {
  padding: 0px 15px 0px 15px; }
  .pagemain .list .child {
    margin-bottom: 15px; }
    .pagemain .list .child .img_con {
      margin-right: 15px; }
    .pagemain .list .child .info .title {
      color: #111111;
      font-size: 16px;
      margin: 3px 0px 8px 0px; }
    .pagemain .list .child .info .des {
      color: #acacac;
      font-size: 12px; }
    .pagemain .list .child .info .price {
      color: #ffc430;
      font-size: 16px;
      font-weight: 600;
      margin-bottom: 3px; }

.pagemain .order_list {
  background: #faf8f8; }
  .pagemain .order_list .order_list_inner {
    padding: 15px 15px 5px 15px; }
    .pagemain .order_list .order_list_inner .child {
      border-radius: 4px;
      background: #fff;
      box-shadow: 0px 0px 10px 3px rgba(127, 127, 127, 0.1);
      margin-bottom: 15px; }
      .pagemain .order_list .order_list_inner .child .order_title {
        margin: 12px 15px 0px 15px;
        padding-bottom: 12px;
        color: #333333;
        font-size: 14px;
        border-bottom: 1px solid #e6e6e6; }
      .pagemain .order_list .order_list_inner .child .order_body {
        margin: 12px 15px 0px 15px;
        border-bottom: 1px solid #e6e6e6; }
        .pagemain .order_list .order_list_inner .child .order_body .body_inner {
          margin-bottom: 10px; }
        .pagemain .order_list .order_list_inner .child .order_body .left {
          font-size: 14px;
          color: #333333;
          width: 75px; }
        .pagemain .order_list .order_list_inner .child .order_body .right {
          font-size: 14px;
          color: #666666; }
      .pagemain .order_list .order_list_inner .child .order_price {
        margin: 10px 15px 0px 15px;
        padding-bottom: 10px;
        border-bottom: 1px solid #e6e6e6; }
        .pagemain .order_list .order_list_inner .child .order_price .left {
          font-size: 14px;
          color: #333333; }
        .pagemain .order_list .order_list_inner .child .order_price .right {
          font-size: 14px;
          color: #ffc430;
          font-weight: 600; }
      .pagemain .order_list .order_list_inner .child .order_pay {
        margin: 10px 15px 10px 15px; }
        .pagemain .order_list .order_list_inner .child .order_pay .stasus {
          font-size: 14px;
          color: #666666; }
        .pagemain .order_list .order_list_inner .child .order_pay .btn {
          font-size: 14px;
          color: #ffc430;
          border: 1px solid #ffc430;
          padding: 6px 14px;
          border-radius: 15px; }

.pagemain .tips {
  color: #ffdc99;
  font-size: 16px;
  text-align: center;
  margin-top: 25px;
  margin-bottom: 25px; }

.pagemain .bar {
  position: absolute;
  bottom: 0px;
  width: 100%;
  height: 55px;
  border-top: 1px solid #ebebeb;
  font-size: 10px; }
  .pagemain .bar .normal {
    color: #000; }
  .pagemain .bar .cur {
    color: #ff9600; }

.pagemain .map_fade {
  position: fixed;
  top: 0px;
  background: rgba(0, 0, 0, 0.7); }
  .pagemain .map_fade .pannel {
    background: #fff;
    border-radius: 4px 4px 0px 0px; }
  .pagemain .map_fade .map_close {
    padding: 15px; }
